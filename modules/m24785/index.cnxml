      <para id="id4718424">Beslissingsblok</para>
      <para id="id5125504">Ja/Nee</para>
      <figure id="id1167813051681">
        <media id="id1167813051681_media" alt="">
          <image mime-type="image/png" src="../../media/Picture 7-618a.png" id="id1167813051681__onlineimage" height="661" width="633"/>
